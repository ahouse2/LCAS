{
  "case_name": "",
  "source_directory": "",
  "target_directory": "",
  "plugins_directory": "plugins",
  "enabled_plugins": [
    "file_ingestion_plugin",
    "hash_generation_plugin",
    "evidence_categorization_plugin",
    "ai_integration_plugin",
    "timeline_analysis_plugin",
    "pattern_discovery_plugin",
    "report_generation_plugin"
  ],
  "debug_mode": false,
  "log_level": "INFO",
  "analysis_settings": {
    "preserve_originals": true,
    "generate_hashes": true,
    "enable_ai_analysis": false,
    "max_files_per_analysis": 1000,
    "timeout_per_plugin": 300
  },
  "folder_structure": {
    "CASE_SUMMARIES_AND_RELATED_DOCS": {
      "keywords": ["summary", "case", "pleading", "motion", "brief"],
      "AUTHORITIES": {},
      "DETAILED_ANALYSIS_OF_ARGUMENTS": {},
      "STATUTES": {}
    },
    "CONSTITUTIONAL_VIOLATIONS": {
      "keywords": ["constitutional", "due_process", "rights", "amendment"],
      "PEREMPTORY_CHALLENGE": {}
    },
    "ELECTRONIC_ABUSE": {
      "keywords": ["spyware", "electronic", "surveillance", "digital", "monitoring"]
    },
    "FRAUD_ON_THE_COURT": {
      "keywords": ["fraud", "deception", "misrepresentation", "perjury"],
      "ATTORNEY_MISCONDUCT_MARK": {},
      "CURATED_TEXT_RECORD": {},
      "EVIDENCE_MANIPULATION": {},
      "EVIDENCE_OF_SOBRIETY": {},
      "EX_PARTE_COMMUNICATIONS": {},
      "JUDICIAL_MISCONDUCT": {},
      "NULL_AGREEMENT": {},
      "PHYSICAL_ASSAULTS_AND_COERCIVE_CONTROL": {}
    },
    "NON_DISCLOSURE_FC2107_FC2122": {
      "keywords": ["financial", "asset", "disclosure", "fc2107", "fc2122", "property"]
    },
    "PD065288_COURT_RECORD_DOCS": {
      "keywords": ["court", "record", "filing", "docket"]
    },
    "POST_TRIAL_ABUSE": {
      "keywords": ["harassment", "violation", "abuse", "contempt"]
    },
    "TEXT_MESSAGES": {
      "keywords": ["text", "message", "sms", "communication"],
      "SHANE_TO_FRIENDS": {},
      "SHANE_TO_KATHLEEN_MCCABE": {},
      "SHANE_TO_LISA": {},
      "SHANE_TO_MARK_ZUCKER": {},
      "SHANE_TO_RHONDA_ZUCKER": {}
    },
    "FOR_HUMAN_REVIEW": {},
    "VISUALIZATIONS_AND_REPORTS": {}
  },
  "ai_integration": {
    "enabled": false,
    "provider": "openai",
    "model": "gpt-4",
    "api_key": "",
    "base_url": "",
    "temperature": 0.1,
    "max_tokens": 4000,
    "timeout": 60
  },
  "plugin_settings": {
    "file_ingestion_plugin": {
      "preserve_structure": true,
      "backup_directory": "00_ORIGINAL_FILES_BACKUP"
    },
    "hash_generation_plugin": {
      "algorithm": "sha256",
      "generate_report": true
    },
    "evidence_categorization_plugin": {
      "auto_categorize": true,
      "confidence_threshold": 0.5
    },
    "timeline_analysis_plugin": {
      "extract_dates": true,
      "classify_events": true,
      "minimum_confidence": 0.3
    },
    "pattern_discovery_plugin": {
      "find_name_patterns": true,
      "find_communication_patterns": true,
      "find_timeline_patterns": true,
      "similarity_threshold": 0.3
    },
    "report_generation_plugin": {
      "include_summary": true,
      "include_details": true,
      "include_visualizations": true,
      "export_formats": ["txt", "json"]
    }
  }
}
